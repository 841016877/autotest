<?xml version="1.0" encoding="UTF-8"?>
<suite name="优测教育-PC端">
  <parameter name="filePathParam"  value="/data/interface/data/app_testcase.xlsx"/>
  
  <!-- Test -->
  <test name="优测教育-测试用例">
    <classes>
      <class name="com.youceedu.interf.test.TestRun"/>
    </classes>
  </test> 
 
  <listeners>
  	  <!-- reportng报告替代testng默认报告 -->
      <listener class-name="org.uncommons.reportng.HTMLReporter"/>
      <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
      
      <!-- 用例执行错误进行重试 -->
      <listener class-name="com.youceedu.interf.listener.AnnotationTransformerImpl"/>
      
      <!-- 去除reportng报告重试的用例 -->
      <listener class-name="com.youceedu.interf.listener.TestListenerImpl"/>
  </listeners>
  
</suite> <!-- Suite -->